    "react-dom": "18.3.1",
    "react-i18next": "15.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.10.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@types/luxon": "3.4.2",
    "@types/react": "18.3.7",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.6.0",
