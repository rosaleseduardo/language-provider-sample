    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.3.0",
    "globals": "15.9.0",
    "husky": "9.1.6",
    "jsdom": "25.0.0",
    "lint-staged": "15.2.10",
    "typescript": "5.6.2",
    "typescript-eslint": "8.6.0",
